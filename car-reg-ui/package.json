    "@types/react-redux": "^7.1.8",
    "axios": "^0.19.2",
    "eslint": "^6.6.0",
    "jest-junit": "^10.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "set CI=false&&react-scripts build",
    "test": "react-scripts test",
    "ciTest": "set CI=true&&react-scripts test --env=jsdom --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject"
